	while (1) {
		if ( DataRead != TeclaOld ) {
			xil_printf (" Se ha leido %d del registro 0 del teclado \n\r" , DataRead );
			TeclaOld = DataRead ;
		}
		DataRead = TECLADO_mReadReg ( XPAR_TECLADO_0_S00_AXI_BASEADDR , 0);
