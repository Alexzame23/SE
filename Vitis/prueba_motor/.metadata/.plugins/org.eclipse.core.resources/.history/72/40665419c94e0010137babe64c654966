#define XPAR_MOTOR_0_S00_AXI_BASEADDR 0x44A00000
#define MOTOR_S00_AXI_SLV_REG0_OFFSET 0
int main () 
{
	u32 data;
	data = 0x9E000000 ;
	MOTOR_mWriteReg ( MOTOR_HW, BASEaddress , 0, Data );
